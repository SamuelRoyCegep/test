package pool.product;

import java.awt.Color;

public class MediumPool extends AboveGroundPool 
{
	public static final int basePrice = 4000;
	public static final int size = 21;
	
	public MediumPool(Color color)
	{
		super(color);
	}
	public double calculatePrice()
	{
		return basePrice;
	}
	public int getSize()
	{
		return size;
	}
}
