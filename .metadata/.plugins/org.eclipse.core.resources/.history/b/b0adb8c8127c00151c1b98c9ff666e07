package piscine.produit;

import java.awt.Color;


public class BigPool extends Pool 
{
	public static final int basePrice = 5000;
	private static final int size = 28;
	
	public BigPool(Color color)
	{
		
	}
	public int calculatePrice()
	{
		return basePrice;
	}
	public int getSize()
	{
		return size;
	}

}
