package pool.product;

import java.awt.Color;

public class SmallPool extends Pool
{
	public static final int basePrice = 3000;
	private static final int size = 15;
	
	public SmallPool(Color color)
	{
		
	}
	public int calculatePrice()
	{
		return basePrice;
	}
	public int getSize()
	{
		return size;
	}
}