package pool.product;

import java.awt.Color;

public class MediumPool extends Pool 
{
	public static final int basePrice = 4000;
	private static final int size = 21;
	
	public MediumPool(Color color)
	{
		
	}
	public int calculatePrice()
	{
		return basePrice;
	}
	public int getSize()
	{
		return size;
	}
}
